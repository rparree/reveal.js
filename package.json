{
  "name": "reveal.js",
  "version": "3.8.0",
  "description": "The HTML Presentation Framework",
  "homepage": "http://revealjs.com",
  "subdomain": "revealjs",
  "main": "js/reveal.js",
  "scripts": {
    "test": "grunt test",
    "start": "grunt serve",
    "build": "grunt",
    "webyarns:build:babel": "babel  --extensions '.ts' webyarns-util/src -d webyarns-util/lib",
    "webyarns:build:minify1": "uglifyjs webyarns-util/lib/webyarn-plugin.js --compress --mangle -o webyarns-util/lib/webyarn-plugin.min.js",
    "webyarns:build:minify2": "uglifyjs webyarns-util/lib/webyarns-sound.js --compress --mangle -o webyarns-util/lib/webyarns-sound.min.js",
    "webyarns:build:minify3": "uglifyjs webyarns-util/lib/audio-ios-initialiser.js --compress --mangle -o webyarns-util/lib/audio-ios-initialiser.min.js",
    "webyarns:build": "npm-run-all webyarns:build:*",
    "webyarns:watch": "babel --extensions '.ts'  webyarns-util/src -d webyarns-util/lib --watch --source-maps inline",
    "test-webyarns": "jest",
    "tsc-webyarns": "tsc"
  },
  "author": {
    "name": "Hakim El Hattab",
    "email": "hakim.elhattab@gmail.com",
    "web": "http://hakim.se"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hakimel/reveal.js.git"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/node": "^12.11.1",
    "@types/reveal": "^3.3.33",
    "express": "^4.16.2",
    "grunt": "^1.0.4",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-connect": "^2.0.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-contrib-jshint": "^2.0.0",
    "grunt-contrib-qunit": "^3.1.0",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-sass": "^3.0.2",
    "grunt-zip": "~0.17.1",
    "jest": "^24.9.0",
    "load-grunt-tasks": "^4.0.0",
    "mustache": "^2.3.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "socket.io": "^2.2.0",
    "typescript": "^3.6.4",
    "uglify-js": "^3.6.5"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@types/howler": "^2.1.1",
    "howler": "^2.1.2"
  }
}
